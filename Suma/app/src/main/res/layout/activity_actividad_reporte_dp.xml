<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActividadReporteDP">

    <TextView
        android:id="@+id/getNombreTxtV"
        android:layout_width="273dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="52dp"
        android:layout_marginLeft="52dp"
        android:layout_marginTop="35dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/getApellidoTxtV"
        android:layout_width="271dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/getNombreTxtV"
        android:layout_alignParentTop="true"
        android:layout_marginTop="79dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/getCedulaTxtV"
        android:layout_width="273dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/getNombreTxtV"
        android:layout_alignParentTop="true"
        android:layout_marginTop="124dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/getEdadTxtV"
        android:layout_width="274dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/getNombreTxtV"
        android:layout_alignParentTop="true"
        android:layout_marginTop="178dp"
        android:text="TextView" />
</RelativeLayout>